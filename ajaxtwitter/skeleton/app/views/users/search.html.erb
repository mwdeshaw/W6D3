<h1>Search!</h1>

<nav class="users-search">
<input class="input" type="text" name="query" value="<%= @query %>">
<input type='submit' value='search'>
<ul class="users-ul">
</ul>
</nav>

<ul>
  <% @users.each do |user| %>
    <li>
      <a href="<%= user_url(user) %>"><%= user.username %></a>
      <%= render 'follows/form', user: user %>
    </li>
  <% end %>
</ul>

